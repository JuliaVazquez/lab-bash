git clone https://github.com/ironhack-labs/lab-bash.git
echo "Hello World"
mkdir new_dir
rmdir new_dir
cd lorem
cp sed.txt ../lorem-copy
cp at.txt ..lorem-copy ; cp lorem.txt ../lorem-copy
cat sed.txt
cat at.txt ; cat lorem.txt
cd ..
cd lorem-copy
head -3 sed.txt
tail -3 sed.txt
echo "Homo homini lupus." >> sed.txt
tail -3 sed.txt
cd ..
cd lorem
sed -i "s/et/ET/g" at.txt
cat at.txt
whoami
pwd
ls *txt
cat sed.txt | wc -l
cd ..
ls -dc lorem* | wc -l
